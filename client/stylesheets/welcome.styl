html,
body
  margin: 0
  padding: 0
  font-family: 'Roboto', sans-serif

.header
  width: 770px
  margin: 20px auto

.content-container
  background: #163C8E
  margin-top: 150px
  padding: 20px

.content
  width: 770px
  margin: 0 auto
  position: relative
  color: white

h1, h2
  font-weight: 400
  line-height: 1
  margin-top: 0
  margin-bottom: 0

h1
  font-size: 60px
  line-height: 55px
  margin-bottom: 10px
  text-transform: uppercase

h2
  line-height: 45px
  font-size: 45px
  font-weight: 100


.logo
  width: 85px
  height: 85px
  position: absolute
  left: -110px
  top: 14px
  color: white


